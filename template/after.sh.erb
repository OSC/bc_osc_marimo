<% if context.cluster !~ /kubernetes/ -%>
# Wait for the server to start
echo "Waiting for marimo server to open port ${port}..."
if wait_until_port_used "${host}:${port}" 600; then
  echo "Discovered marimo server listening on port ${port}!"
else
  echo "Timed out waiting for marimo server to open port ${port}!"
  clean_up 1
fi
sleep 2
<% end -%>
